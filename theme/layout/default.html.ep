<head>
   %= include 'site/head_common.html.ep'
</head>

<body>

   <!-- Header
   ================================================== -->
   <header id="top">

      <div class="row">

         <div class="header-content twelve columns">

            <h1 id="logo-text"><a href="index.html" title=""><%= $site->title %></a></h1>
            % if($site->data->{tagline}) {
            <p id="intro"><%= $site->data->{tagline} %></p>
            % }

         </div>

      </div>

      %= include 'site/nav.html.ep'

   </header> <!-- Header End -->

   <!-- Content
   ================================================== -->
   <div id="content-wrap">

      <div class="row">

         <div id="main" class="eight columns">
            <%= content %>
         </div> <!-- end main -->

         %= include 'site/sidebar.html.ep'

      </div> <!-- end row -->

   </div> <!-- end content-wrap -->


   %= include 'site/footer.html.ep'

   <!-- Java Script
   ================================================== -->
   <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
   <script>window.jQuery || document.write('<script src="/theme/js/jquery-1.10.2.min.js"><\/script>')</script>
   <script type="text/javascript" src="/theme/js/jquery-migrate-1.2.1.min.js"></script>
   <script src="/theme/js/main.js"></script>

</body>

</html>
